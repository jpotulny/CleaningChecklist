package org.trinity_services.cleaningchecklist.models;

public enum ChecklistCategory {

    DUSTING("Dusting"),
    KITCHEN("Kitchen"),
    BATHROOM("Bathroom"),
    BEDROOMS("Bedroom"),
    FLOORING("Flooring"),
    FURNITURE("Furniture"),
    LAUNDRY("LaundryRoom"),
    INTERIOR("Walls"),
    EXTERIOR("ExteriorOfHome");

    private final String value;

    private ChecklistCategory(String s) {
        value=s;
    }

    @Override
    public String toString()
    {
        return value;
    }

}
